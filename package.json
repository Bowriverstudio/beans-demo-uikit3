    "del-cli": "^2.0.0",
    "image-webpack-loader": "^5.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^7.2.0",
    "style-loader": "^0.23.1",
